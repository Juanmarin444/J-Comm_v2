{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "build": {
      "commands": {
        "default": "cd ../client && npm run build && cd .. && cd server && node server.js"
      }
    }
}
  